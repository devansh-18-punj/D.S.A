// Given an array A[] of N integers and an index Key. Your task is to print the element present at index key in the array.

// Input:
// 5 2
// 10 20 30 40 50
// Output:
// 30


// SOLUTION : 

#include<iostream>
#include<vector>
using namespace std;

int finding(vector<int> &arr, int n, int key) {
    for(int i=0; i<n; i++) {
        return arr[key];
        }
}

int main() {
    int n, key;
    cout<<"Enter number of element : ";
    cin>>n;
    cout<<"Enter index of element you want to find : ";
    cin>>key;
    vector<int>arr(n);
    for(int i=0; i<n; i++) {
        cin>>arr[i];
    }
    cout<<finding(arr, n, key);
    return 0;
}
